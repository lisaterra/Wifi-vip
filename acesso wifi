  <script>
    function enviar() {
      const nome = document.getElementById('nome').value.trim();
      const telefone = document.getElementById('telefone').value.trim();
      if (nome === '' || telefone === '') {
        alert('Por favor, preencha todos os campos.');
        return;
      }
      // Aqui Ã© o link da imagem da trolagem
      window.location.href = 'https://lisaterra.github.io/Wifi-vip/trolagem.html';
    }
  </script>
